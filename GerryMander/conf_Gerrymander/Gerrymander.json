{
  "theme": "browser",
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "autoGenInputGraphs": true,
  "storeLogFilesDuration": 7,
  "defCompMode": "split",
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "overwriteSheetOrder": {
    "input": ["foo", "num", "_widgets"]
  },
  "overwriteAliases": {
    "_scalars": {
      "newAlias": "Scalars"
    },
    "nodes": {
      "newAlias": "Fips Code"
    },
    "foo": {
      "newAlias": "Adjacency Data"
    },
    "num": {
      "newAlias": "Votes"
    },
    "assign_result": {
      "newAlias": "Redistricted Map"
    },
    "original_votes": {
      "newAlias": "Original Vote Ratio"
    },
    "new_votes": {
      "newAlias": "Redistricted Vote Ratio"
    },
    "original_win_loss": {
      "newAlias": "original_win_loss"
    },
    "assign_result2": {
      "newAlias": "assigned"
    },
    "assign_result3": {
      "newAlias": "assigned"
    }
  },
  "saveTraceFile": true,
  "roundingDecimals": 2,
  "inputWidgets": {
    "district_num": {
      "widgetType": "slider",
      "alias": "District Number",
      "min": 0,
      "max": 10,
      "default": 2,
      "step": 1,
      "ticks": false,
      "noHcube": false,
      "label": "District Number"
    },
    "forrepub": {
      "widgetType": "dropdown",
      "alias": "Party to Win",
      "choices": ["1", "-1"],
      "selected": "1",
      "noHcube": false,
      "clearValue": false,
      "multiple": false,
      "label": "Party to Win",
      "aliases": ["Redistrict for Republicans", "Redistrict for Democrats"]
    }
  },
  "overwriteHeaderAliases": {
    "nodes": {
      "newHeaders": ["Fips Code", "None"]
    },
    "foo": {
      "newHeaders": ["Fips Code", "Fips Code", "Whether They Are Adjacent"]
    },
    "num": {
      "newHeaders": ["Fips Code", "Parties", "Number of Votes"]
    }
  },
  "dataRendering": {
    "assign_result": {
      "outType": "gerry",
      "packages": ["devtools", "tidyverse", "stringr", "ggplot2"]
    },
    "assign_result2": {
      "outType": "gerryPlusWord",
      "packages": ["devtools", "tidyverse", "stringr", "ggplot2"]
    },
    "assign_result3": {
      "outType": "gerryPlusFips",
      "packages": ["devtools", "ggplot2", "tidyverse", "stringr"]
    }
  },
  "readme": {
    "tabTitle": "Introduction",
    "filename": "readme.md",
    "enableMath": true
  },
  "defaultScenName": "Arizona State",
  "datatable": {
    "class": "display",
    "filter": "bottom",
    "rownames": false,
    "options": {
      "pageLength": 15
    }
  },
  "pageTitle": "Gerrymandering Redistricting APP",
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true
    },
    "height": 700,
    "colWidths": 200
  }
}
