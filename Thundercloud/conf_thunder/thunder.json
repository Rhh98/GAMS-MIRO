{
  "theme": "browser",
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "autoGenInputGraphs": true,
  "storeLogFilesDuration": 7,
  "defCompMode": "split",
  "activateModules": {
    "loadLocal": true,
    "logFile": true,
    "lstFile": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "aggregateWidgets": true,
  "overwriteSheetOrder": {
    "input": ["cloudsinfo", "crcost", "climbdis", "upcost", "downcost", "_widgets"]
  },
  "overwriteAliases": {
    "cloudsinfo": {
      "newAlias": "Information of thunderstorm"
    },
    "crcost": {
      "newAlias": "Cost to cruise"
    },
    "climbdis": {
      "newAlias": "Nautical distance to climb 2k feet"
    },
    "upcost": {
      "newAlias": "Cost of going up 2k feet at upheight"
    },
    "downcost": {
      "newAlias": "Cost of going down 2k feet at downheight"
    },
    "_scalars": {
      "newAlias": "Scalars"
    },
    "_scalars_out": {
      "newAlias": "Output Scalars"
    },
    "flowop": {
      "newAlias": "Flying Path"
    }
  },
  "saveTraceFile": true,
  "roundingDecimals": 2,
  "pivottable": {
    "bgColor": "transparent"
  },
  "inputWidgets": {
    "minheight": {
      "widgetType": "slider",
      "alias": "minheight",
      "min": 0,
      "max": 10,
      "default": 2,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "Minimal flying height /1000ft"
    },
    "offsetcr": {
      "widgetType": "slider",
      "alias": "Cruise distance",
      "min": 0,
      "max": 10,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "Distance to cruise for each decision",
      "default": 1
    },
    "descenddis": {
      "widgetType": "slider",
      "alias": "Nautical distance to descend 2k feet",
      "min": 0,
      "max": 20,
      "default": 10,
      "step": 1,
      "ticks": true,
      "noHcube": false,
      "label": "Nautical distance to descend 2k feet"
    }
  },
  "dataRendering": {
    "flowop": {
      "outType": "fflow"
    }
  },
  "defaultScenName": "default",
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  },
  "readme": {
    "tabTitle": "Readme",
    "filename": "readme.md",
    "enableMath": true
  }
}
