{
  "overwriteSheetOrder": {
    "input": ["mcoord", "rscoord", "min_distance", "cm", "ch", "lowupcoord", "_widgets"]
  },
  "overwriteAliases": {
    "mcoord": {
      "newAlias": "Machine coordinates"
    },
    "rscoord": {
      "newAlias": "Receiving and Shipping coordinates"
    },
    "min_distance": {
      "newAlias": "Minimum distance between different machines"
    },
    "cm": {
      "newAlias": "Cost of moving machines per distance unit"
    },
    "ch": {
      "newAlias": "Cost of moving products per distance unit"
    },
    "lowupcoord": {
      "newAlias": "Lower and upper bound of the coordinates of machines"
    },
    "result": {
      "newAlias": "Layout and Work Flow"
    },
    "pcost": {
      "newAlias": "Total cost of moving 3 products"
    }
  },
  "pivottable": {
    "bgColor": "transparent"
  },
  "theme": "browser",
  "defCompMode": "split",
  "roundingDecimals": 2,
  "activateModules": {
    "logFile": true,
    "lstFile": true,
    "loadLocal": true,
    "attachments": true,
    "downloadTempFiles": false
  },
  "autoGenInputGraphs": true,
  "aggregateWidgets": true,
  "storeLogFilesDuration": 7,
  "excelIncludeMeta": true,
  "excelIncludeEmptySheets": true,
  "saveTraceFile": true,
  "dataRendering": {
    "result": {
      "outType": "res",
      "packages": ["leaflet", "plotly"]
    },
    "pcost": {
      "outType": "cost",
      "packages": "plotly"
    }
  },
  "defaultScenName": "default",
  "readme": {
    "tabTitle": "Readme",
    "filename": "FacLoc.md",
    "enableMath": true
  },
  "scripts": {
    "base": []
  },
  "handsontable": {
    "stretchH": "none",
    "readonly": false,
    "highlightCol": true,
    "highlightRow": true,
    "columnSorting": true,
    "manualColumnResize": true,
    "contextMenu": {
      "enabled": true,
      "allowRowEdit": true,
      "allowColEdit": true,
      "allowReadOnly": false
    },
    "height": 700,
    "colWidths": 150
  }
}
